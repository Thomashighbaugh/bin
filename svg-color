#!/bin/env bash

for file in $*
do
sed -e "s/rgb(36.862746%,20.784314%,69.411767%)/rgb(0,133,174)/g" $file > temp$file
mv temp$file $file
echo "Change Callout -5 " 
echo 
 echo $DATE 
 done


